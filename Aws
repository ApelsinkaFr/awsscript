RaidSection:NewToggle("Авто перезаход", "", function(v)
_G.Rejoin = v
while true do
    if not _G.Rejoin then return end
    wait()
local args = {
    [1] = "Hub"
}

game:GetService("ReplicatedStorage").Remote.Player.Teleport:FireServer(unpack(args))
wait(70)

end
end)

RaidSection:NewToggle("NINJA RAID COLLECT", "", function(v)
