loadstring(game:HttpGet("https://raw.githubusercontent.com/bunny42312/script/main/acs"))()

wait(2)

local Library =   loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
  local Window = Library.CreateLib("Апрель", "Ocean")
  local Main = Window:NewTab("Орбы")
  local Other = Window:NewTab("Машины")
  local Raid = Window:NewTab("Рейды")
local MainSection = Main:NewSection("Открытия")
local OtherSection = Other:NewSection("Машины")
local RaidSection = Raid:NewSection("Рейды")
  local Uis = game:GetService("UserInputService")
Uis.InputBegan:Connect(function(input, index)
    if input.KeyCode == Enum.KeyCode.LeftControl then
        Library:ToggleUI()
    end
end)

MainSection:NewToggle("Демон слеер", "", function(v)
_G.open = v
while true do
    if not _G.open then return end
    wait(0.1)
local args = {
    [1] = workspace.Worlds.DemonSlayer.DemonOrb,
    [2] = 7
}

game:GetService("ReplicatedStorage").Remote.Orbs.OpenOrbs:FireServer(unpack(args))
end
end)

MainSection:NewToggle("Драгон бол", "", function(v)
_G.open2 = v
while true do
    if not _G.open2 then return end
    wait(1)
local args = {
    [1] = workspace.Worlds.DBZ.DBZOrb,
    [2] = 7
}

game:GetService("ReplicatedStorage").Remote.Orbs.OpenOrbs:FireServer(unpack(args))

end
end)

MainSection:NewToggle("Ван Пис", "", function(v)
_G.open3 = v
while true do
    if not _G.open3 then return end
    wait(1)
local args = {
    [1] = "OnePieceOrb",
    [2] = 4
}

game:GetService("ReplicatedStorage").Remote.Orbs.OpenOrbs:FireServer(unpack(args))

end
end)

MainSection:NewToggle("Наруто", "", function(v)
_G.open4 = v
while true do
    if not _G.open4 then return end
    wait(1)
local args = {
    [1] = "NarutoOrb",
    [2] = 4
}

game:GetService("ReplicatedStorage").Remote.Orbs.OpenOrbs:FireServer(unpack(args))

end
end)

OtherSection:NewButton("Открыть пассивки", "", function()
game:GetService("Players").LocalPlayer.PlayerGui.MainGui.Windows.PassiveMachine.Visible = true
end)

RaidSection:NewToggle("DBZ RAID COLLECT", "", function(v)
_G.DBZ = v
while true do
    if not _G.DBZ then return end
    wait()

local args = {
    [1] = "DBZMedal",
    [2] = 1
}

game:GetService("ReplicatedStorage").Remote.Drops.Items:FireServer(unpack(args))
end
end)

RaidSection:NewToggle("DS RAID COLLECT", "", function(v)
_G.DS = v
while true do
    if not _G.DS then return end
    wait()

local args = {
    [1] = "DemonSlayerMedal",
    [2] = 1
}

game:GetService("ReplicatedStorage").Remote.Drops.Items:FireServer(unpack(args))

end
end)

RaidSection:NewToggle("JOJO RAID COLLECT", "", function(v)
_G.JJ = v
while true do
    if not _G.JJ then return end
    wait()

local args = {
    [1] = "JojoMedal",
    [2] = 1
}

